{
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 150,
    "useTabs": false,
    "importOrder": [
        "^next/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^[./].*(?<!\\.(c|le|sc)ss)$",
        "^@/(.*)$",
        "^[.]/[-a-zA-Z0-9_]+[.](module)[.](css|scss|less)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}